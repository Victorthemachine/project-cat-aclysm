{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@tsconfig/node16/tsconfig.json",
    "display": "Node 16",
    "compilerOptions": {
        "outDir": "./dist",
        "lib": [
            "es2021"
        ],
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "es2021",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [".vscode"]
}